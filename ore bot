									Des = true
								end
							until Des == true or tick() - startTime >= 7
							repeat wait() until game:GetService("Players").LocalPlayer.PlayerGui.GUI.ProgressBars:GetChildren()[1] == nil
						end
						game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
					end
				end
			end
		end
